{
    "markdown": {
        "hardwrap": true,
        "parser": "gfm",
        "tags": [
            "description",
            "examples"
        ]
    },
    "opts": {
        "private": false,
        "protected": false,
        "template": "./node_modules/jaguarjs-jsdoc",
        "tutorials": "../doc"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "source": {
        "include": [
            "../src",
            "README.md"
        ]
    },
    "templates": {
        "applicationName": "noscript",
        "cleverLinks": true,
        "disqus": true,
        "googleAnalytics": false,
        "meta": {
            "title": "",
            "description": "",
            "keyword": ""
        },
        "monospaceLinks": true,
        "openGraph": false,
        "yandexMetrika": 24745298
    }
}
